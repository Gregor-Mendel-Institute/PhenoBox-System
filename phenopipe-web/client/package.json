{
  "name": "phenopipe-web-client",
  "version": "0.0.0",
  "license": "GPL-2.0",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-aot": "ng serve --aot --proxy-config proxy.conf.json",
    "build-prod": "ng build --env=prod --output-path=../dist",
    "build-dev-server": "ng build --env=dev-server --output-path=../dist",
    "build-aot": "ng build --env=prod --aot --output-path=../dist",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "download-schema": "apollo-codegen download-schema http://localhost:5000/graphql_insec --output schema.json",
    "generate-gql-types": "gql2ts schema.json > ./src/app/interfaces/gql_schema.d.ts",
    "download-and-generate-gql-types": "npm run download-schema && npm run generate-gql-types",
    "postinstall": "rm -rf node_modules/@types/async ## hack until AsycIterator issues are resolved"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/platform-server": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@auth0/angular-jwt": "^1.1.0",
    "@swimlane/ngx-datatable": "^11.1.7",
    "apollo-angular": "^1.0.0",
    "apollo-angular-link-http": "^1.0.1",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link-context": "^1.0.3",
    "apollo-link-error": "^1.0.3",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^1.2.3",
    "lodash": "^4.17.4",
    "lodash.filter": "^4.6.0",
    "moment": "^2.17.1",
    "ng2-dnd": "^5.0.2",
    "ngx-bootstrap": "^1.9.1",
    "ngx-toastr": "^8.1.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.2",
    "@types/async": "^2.0.31",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.74",
    "@types/lodash.filter": "^4.6.3",
    "@types/node": "6.0.60",
    "apollo-codegen": "^0.16.5",
    "codelyzer": "~4.0.0",
    "gql2ts": "^1.2.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-html-reporter": "^0.2.2",
    "karma-jasmine": "~1.1.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "^5.0.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "webdriver-manager": "10.2.5"
  }
}
